{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "subtitle": {
      "type": "string"
    },
    "tags": {
      "type": "customField",
      "options": [
        "Vallenato",
        "Caja",
        "Acordeon",
        "Cantante A",
        "Cantante B"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
